$rem
    jobname     test
    jobtype     simulation
	loaddat		true
    DIM         2
	hbar		1
    kT          0.001
    Gamma0      0.05
$end

$grid
	rmin		-1		-20	
	rmax		1		0
	Nr			4000	10000
$end

$simulation
    Ntraj       2000
    mass        14000   55000        
    EndT        30000
    Nstep       450000
    Anastep     100

	#-- init state relateda --#

    prepInit    false
    initTemp    0.5
    vibstate    15
	elestate	0
    initmode    ring	delta
	initravg	0		-15
	initpavg	0		20
$end

$surfaces
    surfnum     2
	cutoff_gamma	0.075

    @surfmode    
		harmonic morse
		harmonic morse
	@end

	@surfpara
		0.896 	0 		0	
		0.011 	-0.011	0.64	-3.5
		0.896 	0 		0.2
		0.055 	0.015	0.4	 	-3
	@end

    @gammamode
		constant step
	@end

	@gammapara
		1
		0	1	-9
	@end

	#-- fef related --#

	bandwidth	1.5
	derange		3e-5	3e-6
$end
